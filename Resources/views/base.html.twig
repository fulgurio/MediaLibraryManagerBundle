<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<title>{% block title %}{% trans %}fulgurio.medialibrarymanager.common.title{% endtrans %}{% endblock %}</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta name="description" content="">
		<meta name="author" content="">
		<link href="{{ asset('bundles/fulguriomedialibrarymanager/css/bootstrap.min.css') }}" rel="stylesheet">
		<style>
			body {
				padding-top: 60px; /* 60px to make the container go all the way to the bottom of the topbar */
			}
		</style>
		<link href="{{ asset('bundles/fulguriomedialibrarymanager/css/bootstrap-responsive.min.css') }}" rel="stylesheet">
{% block css %}{% endblock %}
		<!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
		<!--[if lt IE 9]>
			<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
		<![endif]-->
		<!-- Fav and touch icons -->
{#
		<link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('bundles/fulguriomedialibrarymanager/ico/apple-touch-icon-144-precomposed.png') }}">
		<link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset('bundles/fulguriomedialibrarymanager/ico/apple-touch-icon-114-precomposed.png') }}">
		<link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('bundles/fulguriomedialibrarymanager/ico/apple-touch-icon-72-precomposed.png') }}">
		<link rel="apple-touch-icon-precomposed" href="{{ asset('bundles/fulguriomedialibrarymanager/ico/apple-touch-icon-57-precomposed.png') }}">
		<link rel="shortcut icon" href="{{ asset('bundles/fulguriomedialibrarymanager/ico/favicon.png') }}">
#}
	</head>
	<body>
		<div class="navbar navbar-inverse navbar-fixed-top">
			<div class="navbar-inner">
				<div class="container">
					<a class="brand" href="#">{% trans %}fulgurio.medialibrarymanager.common.title{% endtrans %}</a>
					<div class="nav-collapse collapse">
						<ul class="nav">
							<li{% if app.request.attributes.get('_route') == 'FulgurioMLMHomepage' %} class="active"{% endif %}><a href="{{ path('FulgurioMLMHomepage') }}">{% trans %}fulgurio.medialibrarymanager.common.homepage{% endtrans %}</a></li>
							<li{% if app.request.attributes.get('_route')|slice(0, 18) == 'FulgurioMLM_Music_' %} class="active"{% endif %}><a href="{{ path('FulgurioMLM_Music_List') }}">{% trans %}fulgurio.medialibrarymanager.music.music{% endtrans %}</a></li>
						</ul>
					</div>
				</div>
			</div>
		</div>
		<div class="container">
{% block content %}
{% endblock %}
		</div>
		<script src="{{ asset('bundles/fulguriomedialibrarymanager/js/jquery.min.js') }}"></script>
		<script src="{{ asset('bundles/fulguriomedialibrarymanager/js/bootstrap.min.js') }}"></script>
{% block javascript %}{% endblock %}
	</body>
</html>
