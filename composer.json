{
    "name": "fulgurio/media-library-manager-bundle",
    "type": "symfony-bundle",
    "description": "Own media library manager online with Symfony2 framework",
    "keywords": ["media", "library"],
    "homepage": "http://github.com/fulgurio/MediaLibraryManagerBundle",
    "license": "GPL-1.0-or-later",

    "authors": [
        {
            "name": "Fulgurio",
            "homepage": "http://fulgurio.net"
        }
    ],

    "require": {
        "symfony/framework-bundle": "2.8.*",
        "knplabs/knp-paginator-bundle": "^2.5",
        "vich/uploader-bundle": "^0.14.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "liip/imagine-bundle": "^1.8"
    },

    "require-dev": {
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "knplabs/friendly-contexts": "^0.5.11"
    },

    "autoload": {
        "psr-0": {
            "Fulgurio\\MediaLibraryManagerBundle": ""
        }
    },

    "target-dir": "Fulgurio/MediaLibraryManagerBundle"
